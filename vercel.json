{
  "buildCommand": "",
  "installCommand": "",
  "outputDirectory": ".",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*).pdf",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400, s-maxage=604800" },
        { "key": "Content-Type", "value": "application/pdf" }
      ]
    },
    {
      "source": "/(.*).xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" }
      ]
    }
  ]
}
